#include <iostream>
#include <vector>

using namespace std;

unsigned int ret(bool flag)
{
    if ( flag )
        return -1;
    else
        return static_cast<unsigned int>(-1);
}

size_t ret2(bool flag)
{
    if ( flag )
        return -1;
    else
        return static_cast<size_t>(-1);
}

std::string str_ret(bool flag)
{
    std::string test("test");
    if ( flag )
        return test;
    else
        return "";
}

int main()
{
    { // Test uint64_t and size_t
        cout << "uint64_t : " << static_cast<uint64_t>(-1) << endl;
        cout << "size_t   : " << static_cast<size_t>(-1) << endl;
        cout << "Case 1 : " << ret(true) << endl;
        cout << "Case 2 : " << ret(false) << endl;
        cout << "Case 3 : " << ret2(true) << endl;
        cout << "Case 4 : " << ret2(false) << endl;
    }
    { // Test std::string ret
        cout << "Case 5 : " << str_ret(true) << endl;
        cout << "Case 6 : " << str_ret(false) << endl;
    }

    return 0;
}
