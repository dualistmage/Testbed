#include <iostream>

using namespace std;

int main()
{
    { // atoi
        int i,j,k,l,m;
        std::string no("12"), str("st"), largeNo("99999999999999999999999999999999999999"), minus("-1"), zero("0");
        i = atoi( no.c_str() );
        j = atoi( str.c_str() );
        k = atoi( largeNo.c_str() );
        l = atoi( minus.c_str() );
        m = atoi( zero.c_str() );
        cout << "atoi(" << no << ") : " << i << endl;
        cout << "atoi(" << str << ") : " << j << endl;
        cout << "atoi(" << largeNo << ") : " << k << endl;
        cout << "atoi(" << zero<< ") : " << m << endl;
    }
}
