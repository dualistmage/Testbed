#include <iostream>

#include <wiselib/ustring/UString.h>

using std::cout;
using std::endl;

int main()
{
    wiselib::UString input("123 abc jkl !@# {hello world }", wiselib::UString::UTF_8);
    wiselib::UString token1("abc", wiselib::UString::UTF_8);
    wiselib::UString token2("!@#", wiselib::UString::UTF_8);

    size_t pos1 = input.find( token1, 0 );
    size_t pos2 = input.find( token2, 0 );

    std::cout << pos1 << " " << pos2 << endl;
}
